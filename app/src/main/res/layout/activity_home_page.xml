<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/home_main_id"
    android:layout_height="match_parent"
    tools:context=".home_page">

    <FrameLayout
        android:id="@+id/framelayout_id"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/botton_nav_id"
        />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botton_nav_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:menu="@menu/bottm_nav_menu"
        app:itemTextColor="@color/brown"
        app:itemPaddingTop="15dp"
        app:elevation="50dp"
        android:elevation="70dp"
        app:itemPaddingBottom="5dp"
        android:layout_alignParentBottom="true"
        />

</RelativeLayout>